<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:granite="http://www.adobe.com/jcr/granite/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="cq:Page">
    <jcr:content jcr:primaryType="nt:unstructured">
        <actions jcr:primaryType="nt:unstructured">
            <selection jcr:primaryType="nt:unstructured">
                <create
                    granite:class="foundation-collection-action"
                    granite:rel="cq-siteadmin-admin-actions-create-activator"
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="granite/ui/components/coral/foundation/pulldown"
                    icon="addCircle"
                    text="Create"
                    variant="actionBar">
                    <granite:data
                        jcr:primaryType="nt:unstructured"
                        foundation-collection-action="\{&quot;target&quot;: &quot;.cq-siteadmin-admin-childpages&quot;, &quot;activeSelectionCount&quot;: &quot;multiple&quot;}"/>
                    <items jcr:primaryType="nt:unstructured">
                        <createcatalog
                            granite:comment="The actionlink uses the URI derived from the `href.uritemplate` value on the `data` node. The `href` attribute is  only used externally (e.g. HApi tests), not by the actionlink itself"
                            granite:rel="cq-siteadmin-admin-createcatalog"
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="granite/ui/components/coral/foundation/collection/actionlink"
                            action="foundation.link"
                            href="/mnt/overlay/commerce/gui/content/v2/catalogs/createcatalogwizard.html${requestPathInfo.suffix}"
                            icon="fileTemplate"
                            rel="cq-siteadmin-admin-createcatalog"
                            relScope="collection"
                            target=".cq-siteadmin-admin-childpages"
                            text="Catalog">
                            <data
                                jcr:primaryType="nt:unstructured"
                                href.uritemplate="/mnt/overlay/commerce/gui/content/v2/catalogs/createcatalogwizard.html{+id}"/>
                        </createcatalog>
                        <createworkflow
                            granite:rel="cq-siteadmin-admin-createworkflow"
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="granite/ui/components/coral/foundation/collection/actionlink"
                            action="foundation.link"
                            activeSelectionCount="multiple"
                            icon="workflow"
                            target=".cq-siteadmin-admin-childpages"
                            text="Workflow">
                            <granite:rendercondition
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="cq/gui/components/renderconditions/canreadworkflowmodels"/>
                            <data
                                jcr:primaryType="nt:unstructured"
                                href.uritemplate="/mnt/override/libs/wcm/core/content/common/startbulkworkflows.html{?item*}"/>
                        </createworkflow>
                        <createversion
                            granite:rel="cq-siteadmin-admin-createversion"
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="granite/ui/components/coral/foundation/collection/actionlink"
                            action="foundation.dialog"
                            activeSelectionCount="multiple"
                            icon="layers"
                            target=".cq-siteadmin-admin-childpages"
                            text="Version">
                            <data
                                jcr:primaryType="nt:unstructured"
                                nesting="hide"
                                src.uritemplate="/mnt/overlay/wcm/core/content/coral/common/createversiondialog.html"/>
                        </createversion>
                        <createpage
                            granite:comment="The actionlink uses the URI derived from the `href.uritemplate` value on the `data` node. The `href` attribute is  only used externally (e.g. HApi tests), not by the actionlink itself"
                            granite:rel="cq-siteadmin-admin-createpage"
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="granite/ui/components/coral/foundation/collection/actionlink"
                            action="foundation.link"
                            activeSelectionCount="single"
                            href="/mnt/overlay/wcm/core/content/sites/createpagewizard.html${requestPathInfo.suffix}"
                            icon="experience"
                            rel="cq-siteadmin-admin-createpage"
                            relScope="collection"
                            target=".cq-siteadmin-admin-childpages"
                            text="Page"
                            trackingElement="create page"
                            trackingFeature="aem:sites:pages:create">
                            <data
                                jcr:primaryType="nt:unstructured"
                                href.uritemplate="/mnt/overlay/wcm/core/content/sites/createpagewizard.html{+item}"/>
                        </createpage>
                        <createsite
                            granite:comment="The actionlink uses the URI derived from the `href.uritemplate` value on the `data` node. The `href` attribute is  only used externally (e.g. HApi tests), not by the actionlink itself"
                            granite:rel="cq-siteadmin-admin-createsite"
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="granite/ui/components/coral/foundation/collection/actionlink"
                            action="foundation.link"
                            activeSelectionCount="single"
                            href="/mnt/overlay/wcm/core/content/sites/createsitewizard.html${requestPathInfo.suffix}"
                            icon="selectContainer"
                            rel="cq-siteadmin-admin-createsite"
                            relScope="collection"
                            target=".cq-siteadmin-admin-childpages"
                            text="Site">
                            <data
                                jcr:primaryType="nt:unstructured"
                                href.uritemplate="/mnt/overlay/wcm/core/content/sites/createsitewizard.html{+id}"/>
                        </createsite>
                        <createlivecopy
                            granite:rel="cq-siteadmin-admin-createlivecopy"
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="granite/ui/components/coral/foundation/collection/actionlink"
                            action="foundation.link"
                            activeSelectionCount="single"
                            icon="pages"
                            target=".cq-siteadmin-admin-childpages"
                            text="Live Copy"
                            trackingElement="create live copy"
                            trackingFeature="aem:sites:livecopy:create">
                            <data
                                jcr:primaryType="nt:unstructured"
                                href.uritemplate="/mnt/overlay/wcm/core/content/sites/createlivecopywizard.html{+id}?source={item}"/>
                        </createlivecopy>
                        <createlaunch
                            granite:rel="cq-siteadmin-admin-createlivecopy"
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="granite/ui/components/coral/foundation/collection/actionlink"
                            action="foundation.link"
                            activeSelectionCount="multiple"
                            icon="launch"
                            target=".cq-siteadmin-admin-childpages"
                            text="Launch">
                            <data
                                jcr:primaryType="nt:unstructured"
                                href.uritemplate="/mnt/overlay/wcm/core/content/sites/createlaunchwizard.html{+id}?source={item}"/>
                        </createlaunch>
                        <createlanguagecopy
                            granite:rel="cq-siteadmin-admin-createlanguagecopy"
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="granite/ui/components/coral/foundation/collection/actionlink"
                            action="foundation.link"
                            activeSelectionCount="multiple"
                            icon="project"
                            target=".cq-siteadmin-admin-childpages"
                            text="Language Copy">
                            <data
                                jcr:primaryType="nt:unstructured"
                                href.uritemplate="/mnt/overlay/wcm/core/content/sites/createlanguagecopywizard.html?source={item}"/>
                            <granite:rendercondition
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="cq/translation/languagecopyrendercondition"/>
                        </createlanguagecopy>
                        <createfolder
                            granite:comment="The actionlink uses the URI derived from the `href.uritemplate` value on the `data` node. The `href` attribute is  only used externally (e.g. HApi tests), not by the actionlink itself"
                            granite:rel="cq-siteadmin-admin-createfolder"
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="granite/ui/components/coral/foundation/collection/actionlink"
                            action="foundation.dialog"
                            activeSelectionCount="single"
                            href="/mnt/overlay/wcm/core/content/coral/common/createfolderdialog.html${requestPathInfo.suffix}"
                            icon="folder"
                            rel="cq-siteadmin-admin-createfolder"
                            relScope="collection"
                            target=".cq-siteadmin-admin-childpages"
                            text="Folder">
                            <data
                                jcr:primaryType="nt:unstructured"
                                nesting="hide"
                                src.uritemplate="/mnt/overlay/wcm/core/content/coral/common/createfolderdialog.html{+id}"/>
                        </createfolder>
                        <createcsvexport
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="granite/ui/components/coral/foundation/collection/actionlink"
                            action="foundation.link"
                            activeSelectionCount="single"
                            icon="report"
                            rel="cq-siteadmin-admin-createcsvexport"
                            relScope="none"
                            target=".cq-siteadmin-admin-childpages"
                            text="CSV Report">
                            <data
                                jcr:primaryType="nt:unstructured"
                                href.uritemplate="/mnt/overlay/wcm/core/content/sites/createcsvexport.html{+id}"/>
                        </createcsvexport>
                    </items>
                </create>
                <open
                    granite:rel="cq-siteadmin-admin-actions-edit-activator"
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="granite/ui/components/coral/foundation/collection/action"
                    action="cq.wcm.open"
                    actionConfigName="granite/ui/shell/actions/edit"
                    activeSelectionCount="multiple"
                    target=".cq-siteadmin-admin-childpages"
                    variant="actionBar">
                    <data
                        jcr:primaryType="nt:unstructured"
                        cookiePath.url="/"
                        href.uritemplate="/bin/wcmcommand?cmd=open&amp;_charset_=utf-8&amp;path={item}"/>
                </open>
                <viewproperties
                    granite:class="cq-siteadmin-admin-actions-properties-activator"
                    granite:comment="This button requires custom logic to show/hide, hence `activeSelectionCount` is not used"
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="granite/ui/components/coral/foundation/collection/action"
                    action="foundation.link"
                    actionConfigName="granite/ui/shell/actions/properties"
                    target=".cq-siteadmin-admin-childpages"
                    variant="actionBar">
                    <data
                        jcr:primaryType="nt:unstructured"
                        href.uritemplate="/mnt/overlay/wcm/core/content/sites/properties.html{?item*}"/>
                </viewproperties>
                <viewfolderproperties
                    granite:rel="cq-siteadmin-admin-actions-folderproperties-activator"
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="granite/ui/components/coral/foundation/collection/action"
                    action="foundation.link"
                    actionConfigName="granite/ui/shell/actions/properties"
                    activeSelectionCount="single"
                    target=".cq-siteadmin-admin-childpages"
                    variant="actionBar">
                    <data
                        jcr:primaryType="nt:unstructured"
                        href.uritemplate="/mnt/overlay/wcm/core/content/sites/folderproperties.html{+item}"/>
                </viewfolderproperties>
                <openconteniInsight
                    granite:rel="cq-siteadmin-admin-actions-open-content-insight-activator"
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="granite/ui/components/coral/foundation/collection/action"
                    action="foundation.link"
                    activeSelectionCount="single"
                    icon="adobeAnalytics"
                    target=".cq-siteadmin-admin-childpages"
                    text="Analytics &amp; Recommendations"
                    variant="actionBar">
                    <data
                        jcr:primaryType="nt:unstructured"
                        href.uritemplate="/mnt/overlay/cq/contentinsight/content/insight.html{+item}"
                        target="contentinsight"/>
                </openconteniInsight>
                <lockpage
                    granite:rel="cq-siteadmin-admin-actions-lockpage-activator"
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="granite/ui/components/coral/foundation/collection/action"
                    action="cq.wcm.lock"
                    activeSelectionCount="single"
                    icon="lockOn"
                    target=".cq-siteadmin-admin-childpages"
                    text="Lock"
                    variant="actionBar"/>
                <unlockpage
                    granite:rel="cq-siteadmin-admin-actions-unlockpage-activator"
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="granite/ui/components/coral/foundation/collection/action"
                    action="cq.wcm.unlock"
                    activeSelectionCount="single"
                    icon="lockOff"
                    target=".cq-siteadmin-admin-childpages"
                    text="Unlock"
                    variant="actionBar"/>
                <copypage
                    granite:rel="cq-siteadmin-admin-actions-copy-activator"
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="granite/ui/components/coral/foundation/collection/action"
                    action="cq.wcm.copy"
                    actionConfigName="granite/ui/shell/actions/copy"
                    activeSelectionCount="bulk"
                    target=".cq-siteadmin-admin-childpages"
                    trackingFeature="aem:sites:pages:copypaste"
                    variant="actionBar">
                    <granite:data
                        jcr:primaryType="nt:unstructured"
                        foundation-mode-group="cq-siteadmin-admin-childpages"
                        foundation-mode-value="default"/>
                </copypage>
                <movepage
                    granite:rel="cq-siteadmin-admin-actions-move-activator"
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="granite/ui/components/coral/foundation/collection/action"
                    action="cq.wcm.move"
                    actionConfigName="granite/ui/shell/actions/move"
                    activeSelectionCount="single"
                    target=".cq-siteadmin-admin-childpages"
                    variant="actionBar">
                    <data
                        jcr:primaryType="nt:unstructured"
                        href.uritemplate="/mnt/overlay/wcm/core/content/sites/movepagewizard.html{+item}"
                        infoSrc.url="/libs/wcm/core/content/launch.json?_charset_=utf-8{&amp;path}"/>
                </movepage>
                <quickpublish
                    granite:rel="cq-siteadmin-admin-actions-quickpublish-activator"
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="granite/ui/components/coral/foundation/collection/action"
                    action="cq.wcm.quickpublish"
                    activeSelectionCount="multiple"
                    icon="globe"
                    target=".cq-siteadmin-admin-childpages"
                    text="Quick Publish"
                    trackingFeature="aem:sites:pages:publish"
                    variant="actionBar">
                    <data
                        jcr:primaryType="nt:unstructured"
                        referenceSrc.url="/libs/wcm/core/content/reference.json?_charset_=utf-8{&amp;path*}"/>
                </quickpublish>
                <managepublication
                    granite:rel="cq-siteadmin-admin-actions-publish-activator"
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="granite/ui/components/coral/foundation/collection/action"
                    action="foundation.link"
                    icon="globeClock"
                    target=".cq-siteadmin-admin-childpages"
                    text="Manage Publication"
                    trackingFeature="aem:sites:pages:publish"
                    variant="actionBar">
                    <data
                        jcr:primaryType="nt:unstructured"
                        href.uritemplate="/mnt/override/libs/wcm/core/content/common/managepublicationwizard.html{?item*}"/>
                </managepublication>
                <deletepage
                    granite:rel="cq-siteadmin-admin-actions-delete-activator"
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="granite/ui/components/coral/foundation/collection/action"
                    action="cq.wcm.delete"
                    actionConfigName="granite/ui/shell/actions/delete"
                    activeSelectionCount="bulk"
                    target=".cq-siteadmin-admin-childpages"
                    variant="actionBar"/>
                <rollout
                        granite:rel="cq-siteadmin-admin-actions-edit-activator"
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/coral/foundation/collection/action"
                        action="etoolbox.rollout-manager.show-references-dialog"
                        activeCondition="etoolbox.rollout-manager.rollout-active-condition"
                        activeSelectionCount="single"
                        icon="alias"
                        target=".cq-siteadmin-admin-childpages"
                        text="Rollout"
                        variant="actionBar">
                </rollout>
                <updatetranslationmemory
                    granite:rel="cq-siteadmin-admin-actions-translation-update-memory"
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="granite/ui/components/coral/foundation/collection/action"
                    action="foundation.link"
                    activeSelectionCount="multiple"
                    icon="infoCircle"
                    target=".cq-siteadmin-admin-childpages"
                    text="Update Translation Memory"
                    variant="actionBar">
                    <data
                        jcr:primaryType="nt:unstructured"
                        href.uritemplate="/mnt/overlay/cq/gui/components/projects/admin/translation/translationmemoryupdate.html{+item}?path={+item}"/>
                </updatetranslationmemory>
            </selection>
        </actions>
    </jcr:content>
</jcr:root>